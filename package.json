{
  "name": "class-action-finder",
  "version": "1.0.0",
  "description": "A web application for finding and managing class action lawsuits",
  "private": true,
  "author": {
    "name": "Class Action Finder Team",
    "email": "support@classactionfinder.com",
    "url": "https://classactionfinder.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/class-action-finder.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/class-action-finder/issues"
  },
  "homepage": "https://classactionfinder.com",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "cypress run",
    "test:e2e:dev": "cypress open",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "prepare": "husky install",
    "worker:dev": "ts-node-dev --respawn --transpile-only src/worker/index.ts",
    "worker:start": "node dist/worker/index.js",
    "db:migrate": "wrangler d1 migrations apply DB --local",
    "db:seed": "wrangler d1 execute DB --local --file=migrations/0003_seed_data.sql"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.1",
    "@supabase/supabase-js": "^2.39.0",
    "@tanstack/react-query": "^5.0.0",
    "axios": "^1.6.2",
    "bull": "^4.12.0",
    "date-fns": "^2.30.0",
    "elasticsearch": "^16.7.3",
    "express": "^4.18.2",
    "next": "14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.0",
    "redis": "^4.6.11",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.2.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.1.0",
    "@types/bull": "^4.10.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "autoprefixer": "^10.4.16",
    "cypress": "^13.6.0",
    "eslint": "^8.55.0",
    "eslint-config-next": "14.0.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0"
  }
}
